}

/* Side menu */
#slide_menu_container{
	background-color: rgb(177, 177, 177)
}

#menu QPushButton, #exit QPushButton {
	color: #000000;
	text-align: left;
	border-radius: 5px; 
	font-size: 14px;
	font-weight: bold;
	background-color:  rgb(250, 250, 250);
	padding: 5px;
	border: 2px solid #000000;
}

#menu QPushButton:hover, #exit QPushButton:hover {
	color: rgb(255, 255, 255);
	background-color: rgb(193, 193, 193);
	border: 2px solid #ffffff;
	box-shadow: 0 0 10px #00aaff;
}

/* Main board */

#main_body {
	background-color: #F9F9F9; /*#ECF0F1;*/
}

#header_title {
	font-size: 20px;
	font-weight: bold;
	border: none;
}

#header_frame QPushButton {
	border: none;
	padding: 5px;
	background: none;
}

#header_frame QPushButton:hover{
	border: none;
	background: qradialgradient(cx:0.5, cy:0.5, radius:0.6, fx:0.5, fy:0.5, stop:0 rgba(255, 255, 255, 150), stop:1 rgba(255, 255, 255, 0));
}

#dashboard, #setting_frame {
	background-color: #f6f8fc;
	border-radius: 10px;
}

#speed_frame, #temperature_frame, #speed_node, #temperature_node {
	background-color: #ffffff;
	border-radius: 5px;
	border: 1.5px solid #000000;
}

#speed_node, #temperature_node {
	background-color: white;
	border-radius: 5px
}

#speed_text_result, #temperature_text_result, #com_result_text {
	background-color: #61BD4F; 
	color: white;
	border-radius: 5px;
}

#control_btn QPushButton {
	border-radius: 5px;
	font-size: 14px;
	font-weight: bold;
	background-color:  white;
	border: 1px solid #000000
}

#start_btn:hover {
	color: #1abc9c;
	border: 2px solid #1abc9c;
}

#stop_btn:hover {
	color: rgb(226, 0, 5);
	border: 2px solid #EB5A46;
}
/* Setting */

#connect_btn, #uart_save_btn, #uart_change_btn, #uart_cancel_btn, #browse_btn {
	color: #6d718d;
	border-radius: 5px;
	background-color: #ffffff;
	text-align: center;
}

#connect_btn:hover {
	color: #1abc9c;
	border: 1px solid #1abc9c;
}

#uart_save_btn:hover, #uart_change_btn:hover, #browse_btn:hover {
	color: #315ae7;
	border: 1px solid #315ae7;
}

#uart_cancel_btn:hover  {
	color: #EB5A46;
	border: 1px solid #EB5A46;
}

#portList, #baudList, #parity, #stopBit, #byteSize {
	background-color: white;
}

QLineEdit:focus {
	border: 1px solid #315ae7;
}
